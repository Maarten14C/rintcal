Package: rintcal
Type: Package
Title: Radiocarbon Calibration Curves 
Version: 1.2.0
Authors@R: c(
  person("Maarten", "Blaauw", email = "maarten.blaauw@qub.ac.uk",
    role = c("aut", "cre"), comment = c(ORCID = "0000-0002-5680-1515")),
